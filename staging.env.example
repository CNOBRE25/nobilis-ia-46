# NOBILIS-IA - Staging Environment Configuration
# This file contains the environment variables for the staging environment
# Copy this file to .env.staging for staging deployment

# Supabase Configuration (Staging)
VITE_SUPABASE_URL=https://your-staging-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_staging_anon_key_here

# App Configuration
VITE_APP_NAME=NOBILIS-IA (Staging)
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=staging

# Security Configuration (Staging)
VITE_ENABLE_AUDIT_LOGS=true
VITE_SESSION_TIMEOUT=1800
VITE_MAX_LOGIN_ATTEMPTS=5

# Feature Flags (Staging)
VITE_ENABLE_REGISTRATION=true
VITE_ENABLE_PASSWORD_RESET=true
VITE_ENABLE_AI=true
VITE_ENABLE_ANALYTICS=true

# Debug Settings (Staging)
VITE_DEBUG_MODE=true
VITE_DEV_MODE=false

# Optional: Analytics (Staging)
VITE_ANALYTICS_ID=your_staging_analytics_id

# Rate Limiting (Staging)
VITE_API_RATE_LIMIT=150
VITE_LOGIN_RATE_LIMIT=10

# Database Configuration (Staging)
VITE_DB_CONNECTION_LIMIT=20
VITE_DB_TIMEOUT=30000

# Logging Configuration (Staging)
VITE_LOG_LEVEL=info
VITE_LOG_RETENTION_DAYS=90

# Performance Monitoring (Staging)
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_SAMPLE_RATE=0.1

# Error Reporting (Staging)
VITE_ENABLE_ERROR_REPORTING=true
VITE_ERROR_REPORTING_DSN=your_error_reporting_dsn

# Security Headers (Staging)
VITE_FORCE_HTTPS=true
VITE_HSTS_MAX_AGE=31536000
VITE_CSP_REPORT_URI=https://your-domain.com/csp-report

# Staging-specific Settings
VITE_STAGING_BANNER=true
VITE_STAGING_MESSAGE="Ambiente de Homologação - Não utilizar dados reais"
VITE_STAGING_RESET_DATA=true 